<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>JCMSWebTier</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <web-module-libraries>
                <library dirs="200">
                    <file>${libs.RestEasy_2.2.1.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.JSTL_1.1.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.JSF_2.1.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.RichFaces_v4.2.3.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.PrimeFaces-5.0.0.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.hibernate-persistence.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.ejb3-persistence.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.CageCard.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.iText.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.Common.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.RSCommon.classpath}</file>
                </library>
                <library dirs="200">
                    <file>${libs.json-simple.classpath}</file>
                </library>
                <library dirs="100">
                    <file>${reference.JCMSIntegrationTier.dist}</file>
                </library>
                <library dirs="100">
                    <file>${reference.JCMSMiddleTier.dist}</file>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>JCMSIntegrationTier</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
            <reference>
                <foreign-project>JCMSMiddleTier</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
        </references>
    </configuration>
</project>
